
dna_seq1 <- "ATGCATC"
dna_seq2 <- "TCCAG"

seq_length <- nchar(dna_seq1)
paste("Length:",seq_length)

if(seq_length >6){
    paste("Long DNA sequence1")
}else{
    paste("Short DNA sequence1")
}

seq_length <- nchar(dna_seq2)
paste("Length:",seq_length)

if(seq_length >6){
    paste("Long DNA sequence2")
}else{
    paste("Short DNA sequence2")
}

